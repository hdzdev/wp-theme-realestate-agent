{
  "name": "real-estate-agent-theme",
  "version": "1.0.0",
  "description": "WordPress theme for real estate agents",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build:css && npm run build:header",
    "build": "npm run build:css && npm run build:header",
    "build:css": "tailwindcss -i ./assets/css/input.css -o ./style.temp.css --minify",
    "build:header": "powershell -Command \"(Get-Content ./assets/css/theme-header.css) + (Get-Content ./style.temp.css) | Set-Content ./style.css\" && powershell -Command \"Remove-Item ./style.temp.css\"",
    "watch": "tailwindcss -i ./assets/css/input.css -o ./style.temp.css --watch"
  },
  "keywords": [
    "wordpress",
    "theme",
    "real-estate"
  ],
  "author": "",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {}
}

